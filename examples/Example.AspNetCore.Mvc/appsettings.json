{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "OpenTelemetry": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "AllowedHosts": "*",
  "Elastic": {
    "OpenTelemetry": {
      "LogLevel": "Trace",
      "LogDirectory": "C:\\Logs\\BrandNewLogs",
      "LogTargets": "file;stdout"
    }
  }
}
